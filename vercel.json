{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy/[...path].js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy/[...path].js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 